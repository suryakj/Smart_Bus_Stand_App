        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.719" />

    <TextView
        android:id="@+id/textTime"
        android:layout_width="122dp"
        android:layout_height="42dp"
        android:gravity="right"
